#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
#include<stdbool.h>
int main()
{
	int a = 0;
	int sum = 0;
	printf("请输入一个数字");
	scanf("%d\n",&a);
	for (int i = 0; i < a; i++)
	{
		sum += (i + 1);
	}
	printf("%d\n", sum);
	system("pause");
	return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
#include<stdbool.h>
int main()
{
	int a = 0;
	int sum = 0;
	int b = 0;
	//printf("请输入一个数字");
	//scanf("%d\n",&a);
	for (int i = 1; i <= 100; i++)
	{
		if (i % 2 != 0)
		{
			sum += i*i;
		}
	}
	printf("%d\n", sum);
	system("pause");
	return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
#include<stdbool.h>
int main()
{
	int n = 5;
	int sum = 0;
	for (int i = 2; i < n; i++)
	{
		if (n%i != 0)
		{
			sum = sum + 1;
		}

	}
	if (sum == (n-2))
		{
			printf("质数");
		}
	else 
	{ 
		printf("不是质数");
        	}
	system("pause");
	return 0;
}


#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
#include<stdbool.h>
int main()
{
	int count =0;
	for (int i = 100; i <=200 ; i++)
	{
		if (i%7== 0)
		{
			count++;
			if (count<=10)
			{
				printf("%d\n", i);
			}
		}

	}
	system("pause");
	return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
#include<stdbool.h>
int main()
{
	int year = 2100;
	if (year%100==0)
	{
		if (year%400==0)
		printf("%d是闰年",year);
		
		else
		printf("%d不是闰年", year);
	}
	else
	{
		if (year%4==0)
			printf("%d是闰年", year);

		else
	    printf("%d不是闰年", year);
	}
	system("pause");
	return 0;
}