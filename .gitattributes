#include<iostream>
#include<string>
using std::string;
using std::cin;
using std::cout;
using std::endl;
int main()
{
	string s("Hello world!!");
	decltype(s.size()) punct_cnt = 0;
	for (auto c : s)
		if (ispunct(c))
			++punct_cnt;
		cout <<  punct_cnt<<"punctuation characters is in the "<<s<< endl;
	system("pause");
	return 0;

}

#include<iostream>
#include<string>
using std::string;
using std::cin;
using std::cout;
using std::endl;
int main()
{
	string s("Hello world!!");
	for (auto &c : s)
		c = toupper(c);
		cout << s<< endl;
	system("pause");
	return 0;

}

#include<iostream>
#include<string>
using std::string;
using std::cin;
using std::cout;
using std::endl;
int main()
{
	string s("Hello world!!");
	for (
		decltype(s.size()) index = 0;
		index != s.size() && ! isspace (s[index]); ++index)
	s[index] = toupper(s[index]);
			cout << s << endl;
	system("pause");
	return 0;

}

  #include<iostream>
