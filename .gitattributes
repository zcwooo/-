#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
int main()
{
	int count = 0;
	int i;
	for (i = 0; i < 100; i++)
	{
		if (i % 2)
			continue;
		count++;
		printf("%d\n",i);
	}
	printf("%d\n",count);
	system("pause");
	return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
int main()
{
	unsigned char buf[4] = { 1, 2, 3, 13 };
	for (int i = 0; i < 4; i++)
	{
		printf ("%02X\n", buf[i]);
	}
	system("pause");
	return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
int main()
{
	unsigned char buf[8] = { 1, 2, 3, 4,5,6,7,8 };
	for (int i = 0; i < 8; i++)
	{
		printf ("%02X\n", buf[i]);
		if ((i + 1) % 4 == 0)
			printf("\n");
	}
	system("pause");
	return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
int main()
{
	int n=5;
	int buf[5] = { 1, 2.3,4,5};
	for (int i = 0; i < 5 / 2; i++)
	{
		int t = buf[i];
		buf[i]=buf[n - 1 - i];
		buf[n - 1 - i]=t;

	}
	for (int j = 0; j < 5; j++)
	{
		printf("%d\n",buf[j]);
	}
	system("pause");
	return 0;
}

