{
	int x = 87;
	if (x < 60)
	{
		printf("c\n");
	}
	else if (x < 80)
	{
		printf("b\n");
	}
	else
	{
		printf("c\n");
	}
	getchar();
	return 0;
}

#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>
#include<iostream>
int main()
{
	int n = -1;
	printf("请输入字母");
	scanf("%d", &n);

	if (n==0||n==6)
	{
		printf("home\n");
	}
	else if (n>=1&&n<=5)
	{
		printf("work\n");
	}
	else
	{
		printf("wrong");
	}
	system("pause");
	return 0;

}

